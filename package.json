{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.26",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore node_modules src/main/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "debug": "yarn build && yarn watch-debug",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/main/server.ts",
    "resetDatabase": "ts-node-dev ./node_modules/typeorm/cli.js query 'DROP TABLE IF EXISTS appointments, users, migrations' && ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "migration:run": "yarn typeorm migration run",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts'",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --passWithNoTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "commitizen": "^4.2.1",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.62"
  }
}
